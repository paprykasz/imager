{
  "name": "papryk/imager",
  "description": "Test task project",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "php-di/php-di": "^6.4",
    "symfony/http-foundation": "^6.1",
    "symfony/routing": "^6.1",
    "intervention/image": "^2.7",
    "monolog/monolog": "^3.2",
    "php-webdriver/webdriver": "^1.13"
  },
  "config": {
    "platform": {
      "php": "8.1"
    }
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-mbstring": "*",
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.13",
    "jtl/unit-test": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Papryk\\Imager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Papryk\\Imager\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "chmod 777 $PWD/public",
      "chmod 777 $PWD/storage/logs"
    ]
  },
  "authors": [
    {
      "name": "Patryk Gorol",
      "email": "patrykgorol@gmail.com"
    }
  ]
}
